{
  "name": "GitKill",
  "action": {},
  "manifest_version": 3,
  "version": "0.0.0.1",
  "description": "Delete the opened github repository if you're logged",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+E"
      }
    }
  }
}